{
  "name": "wljs-wlx-support",
  "version": "0.1.3",
  "description": "WLX cells support for WLJS Editor",
  "scripts": {
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/rollup --config rollup.config.mjs",
    "restore": "mkdir _temp && cd _temp && git clone --branch dev https://github.com/JerryI/wljs-wlx-support && mv wljs-wlx-support/.git ../.git && cd .. && rm -rf _temp"
  },
  "defaultPackage": true,
  "wljs-meta": {
    "frontend": "src/Frontend.wl",
    "minjs": "src/kernel.js",
    "js": "src/kernel.js",
    "styles": "src/styles.css",
    "priority": 500,
    "category": "Cell types",
    "cellExportRequired": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JerryI/wljs-wlx-support"
  },
  "dependencies": {
    "rollup": "^3.21.6"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
}
